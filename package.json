{
  "private": true,
  "version": "0.1.3",
  "name": "reg-suit-report-ui",
  "scripts": {
    "clean": "rimraf dist",
    "start": "webpack-dev-server --hot",
    "start:huge": "webpack-dev-server --hot --env.ENABLED_HUGE",
    "build": "cross-env NODE_ENV=\"production\" npm-run-all -s clean build:source",
    "build:source": "webpack",
    "typecheck": "tsc",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "lint": "eslint \"**/*.+(ts|tsx)\"",
    "format": "npm-run-all format:*",
    "format:eslint": "yarn lint --fix",
    "format:prettier": "prettier --write \"**/*.+(ts|tsx|js|md|json)\"",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "scaffold": "yarn scaffdog generate"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addons": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/debounce": "^1.2.0",
    "@types/fuzzy-search": "^2.1.0",
    "@types/imagesloaded": "^4.1.1",
    "@types/jest": "^24.0.19",
    "@types/mousetrap": "^1.6.3",
    "@types/node": "^12.7.12",
    "@types/rc-slider": "^8.6.5",
    "@types/react": "^16.9.6",
    "@types/react-dom": "^16.9.2",
    "@types/react-measure": "^2.0.5",
    "@types/react-transition-group": "^4.2.3",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "4.1.8",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.15",
    "rimraf": "^3.0.0",
    "scaffdog": "^0.1.2",
    "typescript": "^3.6.4",
    "typescript-eslint-language-service": "^1.4.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "dependencies": {
    "body-scroll-lock": "^2.6.4",
    "clipboard-polyfill": "^2.8.6",
    "debounce": "^1.2.0",
    "focus-trap": "^5.0.2",
    "fuzzy-search": "^3.0.1",
    "keycode": "^2.2.0",
    "mousetrap": "^1.6.3",
    "rc-slider": "^8.7.1",
    "re-resizable": "^6.1.0",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-md-spinner": "^1.0.0",
    "react-transition-group": "^4.3.0",
    "styled-components": "5.0.0-beta.9",
    "unstated-next": "^1.1.0",
    "use-intersection": "^0.1.0",
    "x-img-diff-js": "^0.3.5"
  },
  "license": "MIT",
  "browserslist": "> 1%, not dead, not IE <= 11, not OperaMini all",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}

{
  "name": "reg-suit-report-ui",
  "version": "0.2.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=\"production\" run-s -s clean build:source",
    "build:source": "webpack",
    "clean": "rimraf dist",
    "format": "run-s format:prettier format:script",
    "format:prettier": "prettier --write .",
    "format:script": "yarn lint:script --fix",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier --check .",
    "lint:script": "eslint . --ext \".js,.jsx,.ts,.tsx\"",
    "scaffold": "yarn scaffdog generate",
    "start": "webpack serve",
    "start:huge": "webpack serve --env ENABLED_HUGE",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,yml,json}": [
      "prettier --write"
    ]
  },
  "browserslist": "> 1%, not dead, not IE <= 11, not OperaMini all",
  "dependencies": {
    "body-scroll-lock": "^3.1.5",
    "clipboard-polyfill": "^3.0.2",
    "debounce": "^1.2.0",
    "focus-trap": "^6.2.3",
    "fuse.js": "^6.4.6",
    "history": "^5.0.0",
    "keycode": "^2.2.0",
    "mousetrap": "^1.6.5",
    "preact": "^10.5.10",
    "rc-slider": "^9.7.1",
    "re-resizable": "^6.9.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-md-spinner": "^1.0.0",
    "react-transition-group": "^4.4.1",
    "styled-components": "5.2.1",
    "unstated-next": "^1.1.0",
    "use-intersection": "^0.2.1",
    "x-img-diff-js": "^0.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addons": "^6.1.14",
    "@storybook/react": "^6.1.14",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/debounce": "^1.2.0",
    "@types/fuzzy-search": "^2.1.0",
    "@types/imagesloaded": "^4.1.2",
    "@types/jest": "^26.0.20",
    "@types/mousetrap": "^1.6.5",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-measure": "^2.0.6",
    "@types/react-transition-group": "^4.4.0",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.0.0-beta.5",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-packagejson": "^2.2.9",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "scaffdog": "^1.0.0",
    "typescript": "^4.1.3",
    "typescript-eslint-language-service": "^4.1.3",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  }
}
